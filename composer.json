{
  "name": "vicent/laque-swahil-locale",
  "description": "Swahili locale helpers: ICU-based dates/numbers, lightweight inflector, terminology dictionary, and optional PSR-15 locale negotiation. Framework-agnostic, SOLID.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vicent Msomi",
      "email": "msomivicent@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Laque\\SwahiliLocale\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "psr/http-message": "^2.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-mbstring": "*",
    "nyholm/psr7": "^1.8",
    "nyholm/psr7-server": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test:smoke": "php bin/smoke.php",
    "test:unit": "vendor/bin/phpunit --colors=always"
  }
}
